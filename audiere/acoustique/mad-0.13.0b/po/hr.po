msgid "  -1, --left                 output first (left) channel only\n"
msgstr "  -1, --left                 autput prvi, samo (ljevi) kanal\n"

#: madplay.c:135
msgid "  -2, --right                output second (right) channel only\n"
msgstr "  -2, --right                autput drugi, samo (desni) kanal\n"

